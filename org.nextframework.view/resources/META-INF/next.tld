	<tag>
		<description>Submit</description>
		<name>submit</name>
		<tag-class>org.nextframework.view.SubmitTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description></description>
			<name>url</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description></description>
			<name>confirmationScript</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
